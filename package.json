{
  "name": "@artdeco/technation.sucks",
  "version": "0.0.0",
  "description": "The technation.sucks website.",
  "main": "index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "frontend": "depack-dev frontend/comments -o closure/comments.js -a -H --package_json_entry_names module --hide_warnings_for preact --externs frontend/comments/externs.js -E",
    "lint": "eslint .",
    "prod": "git push prod",
    "start": "node src",
    "closure": "CLOSURE=1 node src",
    "dev": "node src",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s -i index.js",
    "deploy": "git push api",
    "d": "yarn-s d1",
    "d1": "NODE_DEBUG=doc doc src/index.js -g",
    "build": "yarn-s d b doc",
    "e": "node example",
    "example/": "yarn e example/example.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecoweb/technation.sucks.git"
  },
  "keywords": [
    "technation.sucks",
    "artdeco"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecoweb/technation.sucks/issues"
  },
  "homepage": "https://github.com/artdecoweb/technation.sucks#readme",
  "dependencies": {
    "@depack/render": "^1.3.2",
    "@idio/core": "1.4.0",
    "@idio/linkedin": "1.4.0",
    "@idio/router": "1.3.1",
    "@lemuria/popup": "^1.0.0",
    "@svag/lib": "2.0.0",
    "@svag/window": "1.1.1",
    "catchment": "3.3.0",
    "elasticsearch": "16.3.0",
    "erte": "1.2.0",
    "koa-static-cache": "5.1.2",
    "logarithm": "^1.0.1",
    "uid-safe": "2.1.5"
  },
  "devDependencies": {
    "@a-la/jsx": "1.6.0",
    "@demimonde/dotenv": "1.1.0",
    "@externs/preact": "1.2.1",
    "@typedefs/goa": "^0.0.5",
    "@wrote/clone": "1.2.1",
    "@wrote/ensure-path": "1.1.0",
    "@wrote/read": "1.0.4",
    "@wrote/read-dir-structure": "1.2.0",
    "@wrote/write": "1.0.4",
    "alamode": "2.4.0",
    "preact": "8.5.1",
    "preact-richtextarea": "1.2.1",
    "restream": "3.7.4",
    "rqt": "4.0.0",
    "spawncommand": "2.2.0",
    "temp-context": "2.1.3",
    "zoroaster": "4.1.1-alpha"
  }
}
